package com.test.spring;

public class College {
	
	int id;
	String name;
	String city;
	
	public void setId(int id) {
		this.id = id;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public void setCity(String city) {
		this.city = city;
	}
	
	public void display() {
		System.out.println("College name is "+name+" :: id is "+id+" :: city is "+city);
	}
	
	
}
